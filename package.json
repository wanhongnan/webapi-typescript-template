{
  "name": "fbdata",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "nodemon",
    "debug": "nodemon --config nodemon-debug.json",
    "build prod": "rimraf dist && npm run build",
    "start prod": "node dist/main.js",
    "starthot": "supervisor dist/main.js",
    "clear modules": "rimraf node_modules",
    "server": "node --inspect dist/server",
    "webpack": "rimraf dist && webpack"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.0.2",
    "@nestjs/swagger": "^3.0.2",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "express-rate-limit": "^3.4.0",
    "gridfs-stream": "^1.1.1",
    "json5": "^2.2.2",
    "jsonfile": "^5.0.0",
    "log4js": "^4.0.2",
    "mongodb": "^3.2.2",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/gridfs-stream": "^0.5.30",
    "@types/jest": "^23.3.13",
    "@types/log4js": "^2.3.5",
    "@types/mongodb": "^3.1.22",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "copy-webpack-plugin": "^5.0.2",
    "jest": "^23.6.0",
    "json5-loader": "^2.0.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "typescript": "^3.2.4",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
